    avg_ww=(float)total/n;
    total=0;
    
    for(i=0;i<n;i++)
    {
        tut_t[i]=abs(B_T[i])+abs(w_time[i]);
        total=total + tut_t[i];
        
    }
    avg_tut=(float)total/n;
    printf("DONE\n");
    printf("\n\nAverage waiting time = %.2f",avg_ww);
    printf("\nAverage turnaround time = %.2f\n",avg_tut);
	
}
